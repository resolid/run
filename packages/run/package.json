{
  "name": "@resolid/run",
  "version": "0.1.1",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs",
      "types": "./dist/vite.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {},
  "devDependencies": {
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.0",
    "@types/readable-stream": "^2.3.15",
    "esbuild-plugin-solid": "^0.5.0",
    "readable-stream": "^3.6.2",
    "solid-js": "^1.6.14",
    "undici": "^5.20.0",
    "vite": "^4.2.0",
    "vite-plugin-solid": "^2.6.1"
  },
  "peerDependencies": {
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.0",
    "esbuild-plugin-solid": "^0.5.0",
    "readable-stream": "^3.6.2",
    "solid-js": "^1.6.14",
    "undici": "^5.21.0",
    "vite": "^4.2.0",
    "vite-plugin-solid": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resolid/run",
    "directory": "packages/run"
  },
  "homepage": "https://run.resolid.tech",
  "license": "MIT"
}
