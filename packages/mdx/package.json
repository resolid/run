{
  "name": "@resolid/mdx",
  "version": "0.1.1",
  "description": "MDX support for SolidJS",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "solid-js": "^1.7.3"
  },
  "peerDependencies": {
    "solid-js": ">=1"
  }
}
